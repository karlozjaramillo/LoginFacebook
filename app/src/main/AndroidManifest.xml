<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cj.loginfacebook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginFacebook">
        <activity android:name=".LoginNetflix"></activity>
        <activity android:name=".LoginGoogle" />
        <activity android:name=".LoginTwitter" />
        <activity
            android:name=".ui.login.LoginTwitter"
            android:label="@string/title_activity_login_twitter" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity" />
    </application>

</manifest>